{
  "name": "@dtsgenerator/create",
  "version": "1.8.0",
  "description": "This repository is the create command for dtsgenerator plugin.",
  "main": "./bin/create.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "create": "./bin/create.js"
  },
  "scripts": {
    "format": "prettier --write src/*.ts",
    "lint": "eslint --fix src/*.ts",
    "fix": "npm run format && npm run lint",
    "compile": "tsc -p .",
    "build": "npm run fix && npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dtsgenerator/create.git"
  },
  "keywords": [
    "dtsgenerator"
  ],
  "author": "horiuchi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dtsgenerator/create/issues"
  },
  "homepage": "https://github.com/dtsgenerator/create#readme",
  "dependencies": {
    "commander": "^8.1.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.0.0",
    "mustache": "^4.2.0",
    "tslib": "^2.3.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@dtsgenerator/eslint-config": "^0.2.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/mustache": "^4.1.2",
    "@types/node": "^16.7.2",
    "@types/validate-npm-package-name": "^3.0.3",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "src/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
